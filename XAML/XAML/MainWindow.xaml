        <Label x:Name="contra" Content="Constraseña:" HorizontalAlignment="Left" Margin="100,207,0,0" VerticalAlignment="Top" RenderTransformOrigin="-1.158,-0.538" Grid.RowSpan="2"/>
        <TextBox x:Name="textnombre" HorizontalAlignment="Left" Height="23" Margin="241,73,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="190" Grid.RowSpan="2"/>
